{
    "env": {
        "node": true,
        "browser": false,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "max-len": "off",
        "comma-dangle": ["error", "never"],
        "react/prop-types": [0],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
